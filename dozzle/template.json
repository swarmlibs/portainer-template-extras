{
    "type": 2,
    "title": "Dozzle",
    "name": "dozzle",
    "description": "Real-time logging and monitoring for Docker in the browser",
    "categories": [
        "docker"
    ],
    "platform": "linux",
    "logo": "https://dozzle.dev/logo.svg",
    "repository": {
        "url": "https://github.com/swarmlibs/portainer-template-extras",
        "stackfile": "dozzle/docker-stack.yml"
    },
    "env": [
        {
            "name": "PUBLISHED_PORT",
            "label": "Published port",
            "default": "8080"
        },
        {
            "name": "Log level",
            "label": "DOZZLE_LEVEL",
            "select": [
                {
                    "text": "info",
                    "value": "info",
                    "default": true
                },
                {
                    "text": "debug",
                    "value": "debug"
                },
                {
                    "text": "trace",
                    "value": "trace"
                }
            ]
        }
    ]
}
